{
  "name": "@risadams/time-since",
  "version": "2.1.0",
  "description": "A robust, type-safe utility for calculating and formatting time differences. Supports multiple formats (relative, absolute, or individual time units) with internationalization support and accurate handling of future dates, leap years, and timezone differences.",
  "main": "dist/time-since.cjs.js",
  "module": "dist/time-since.esm.js",
  "types": "dist/time-since.d.ts",
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "dev": "rollup -c -w"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/risadams/time-since.git"
  },
  "keywords": [
    "typescript",
    "javascript",
    "date-time",
    "relative-time",
    "time-difference",
    "elapsed-time",
    "time-calculator",
    "human-readable",
    "i18n",
    "internationalization",
    "zero-dependency",
    "date-formatting",
    "time-ago",
    "date-utility"
  ],
  "author": "Ris Adams <ris@risadams.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/risadams/time-since/issues"
  },
  "files": [
    "dist/**/*.*"
  ],
  "homepage": "https://github.com/risadams/time-since#readme",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.7",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "jest": "^30.2.0",
    "rollup": "^4.52.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  }
}
